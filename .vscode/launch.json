  "version": "0.2.0",
  "configurations": [
    {
      "name": "seedcase-debugger",
      "python": "${workspaceFolder}/.venv/bin/python3",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "internalConsole",
      "args": [
        "runserver"
      ]
    },
    {
      "name": "test-debugger",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "purpose": [
        "debug-test"
      ],
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
